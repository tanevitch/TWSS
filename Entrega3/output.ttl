    rdfs:domain schema:Movie ;
    rdfs:range schema:Movie .

schema:location a owl:FunctionalProperty,
        owl:ObjectProperty ;
    rdfs:domain schema:ScreeningEvent ;
    rdfs:range schema:MovieTheater .

