<http://www.semanticweb.org> a owl:Ontology .

:directs a owl:ObjectProperty ;
    rdfs:domain schema:Person ;
    rdfs:range schema:Movie ;
    owl:inverseOf schema:director .

:hasSequel a owl:ObjectProperty ;
    rdfs:domain schema:Movie ;
    rdfs:range schema:Movie ;
    owl:inverseOf :sequelOf .

:performsIn a owl:ObjectProperty ;
    rdfs:domain schema:Person ;
    rdfs:range schema:Movie ;
    owl:inverseOf schema:actor .

schema:address a owl:DatatypeProperty ;
    rdfs:domain [ a owl:Class ;
            owl:unionOf ( schema:MovieTheater schema:ScreeningEvent ) ] ;
